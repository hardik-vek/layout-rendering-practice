
<center>
<h2>Users Listing</h2>
<table> 
<th>Username</th>
<th>Password</th>
<th>Actions</th>
<% @users.each do |user| %>
<tr>
<td><%= user.username %></td>
<td><%= user.password %></td>
<td><%= button_to "show", user_path(user),method: :get %></td>
</tr>
<% end %>
</table>
<%= link_to "new", new_user_path,method: :get %>
</center>
